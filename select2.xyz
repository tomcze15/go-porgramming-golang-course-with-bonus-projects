package main

import (
	"fmt"
	"time"
)

func main() {
	chInt := make(chan int)
	chFloat := make(chan float32)

	go func() {
		time.Sleep(3 * time.Second)
		chInt <- 1
		fmt.Println("channel <- 1")
	}()

	go func() {
		time.Sleep(3 * time.Second)
		chFloat <- .1
		fmt.Println("channel <- 0.1")
	}()

	fmt.Println("Starting...")

	select {
	case i, ok := <-chInt:
		fmt.Printf("received %v from chInt", i)
		if !ok {
			//return
		}
	case f, ok := <-chFloat:
		fmt.Printf("received %.1f from chFloat", f)
		if !ok {
			//return
		}
	}

	fmt.Println("\n===============================")
}
